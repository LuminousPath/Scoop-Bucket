{
    "version": "3.35.5",
    "description": "A small, fast, self-contained, high-reliability and full-featured SQL database engine. Includes Library files",
    "homepage": "https://www.sqlite.org/",
    "license": "Public Domain",
    "url": [
        "https://www.sqlite.org/2021/sqlite-tools-win32-x86-3350500.zip",
        "https://sqlite.org/2021/sqlite-dll-win32-x86-3350500.zip"
    ],
    "hash": "bfe3832aea1dda2bd7b0fe91ebcd653fa160106593f84ba0ab1d51161c4df65d",
    "extract_dir": "sqlite-tools-win32-x86-3350500",
    "bin": [
        "sqlite3.exe",
        "sqldiff.exe",
        "sqlite3_analyzer.exe"
    ],
    "pre_install": "if ((Get-Command lib -ErrorAction SilentlyContinue) -eq $null) { throw 'You must have Visual Studio Installed and must run this installer from the developer powershell. Please install Visual Studio and make sure that `lib.exe` is in your path' }",
    "installer": {
        "script": "lib /DEF:$dir/sqlite3.def /OUT:$dir/sqlite3.lib /MACHINE:x64"
    },
    "env_set": {
        "SQLITE3_LIB_DIR": "$dir"
    },
    "checkver": {
        "url": "https://www.sqlite.org/download.html",
        "regex": "(?sm)\\d+-bit DLL \\(x\\d+\\) for SQLite version ([\\d.]+)\\..*?(?<year>\\d+)/sqlite-tools-win32-x86-(?<clean>\\d+)"
    },
    "autoupdate": {
        "url": [
            "https://www.sqlite.org/$matchYear/sqlite-tools-win32-x86-$matchClean.zip",
            "https://sqlite.org/2021/sqlite-dll-win32-x86-$matchClean.zip"
        ],
        "extract_dir": "sqlite-tools-win32-x86-$matchClean"
    }
}
